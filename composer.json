{
	"name": "eliuflorez/twitter",
	"description": "Laravel wrapper for the Twitter API.",
	"homepage": "https://github.com/EliuFlorez/twitter",
	"keywords": ["laravel", "twitter", "api"],
	"authors": [
		{
			"name": "Eliu Florez",
			"email": "eliufz@gmail.com"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"illuminate/contracts": "5.0.*|5.1.*",
		"illuminate/support": "5.0.*|5.1.*",
		"graham-campbell/manager": "~2.1",
		"cosenary/instagram": "~2.0"
	},
	"require-dev": {
		"graham-campbell/testbench": "~2.1"
	},
	"autoload": {
		"psr-4": {
			"EliuFlorez\\Twitter\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"EliuFlorez\\Tests\\Twitter\\": "tests/"
		}
	},
	"extra": {
	    "branch-alias": {
	        "dev-master": "2.1-dev"
	    }
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
